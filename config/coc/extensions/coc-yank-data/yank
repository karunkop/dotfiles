bc4a361e27213ce4196b4db751c2a24d|file:///home/prabhat/Documents/code/neesum/neespos/src/api/axiosInterceptor.js|1|1|V|javascript
	import Axios from 'axios';
	import { baseUrl } from '../constants';
	import { getToken } from '../utils/tokenHelper';
	
	// import { showErrorNotification } from '../components/Notification';
	
	export const axiosInstance = Axios.create({
	    baseURL: baseUrl,
	    timeout: 20000,
	});
	
	//add token to all request
	axiosInstance.interceptors.request.use(function (config) {
	    const token = getToken();
	    if (token) {
	        config.headers['Authorization'] = 'Bearer ' + token;
	    }
	    return config;
	});
	
	axiosInstance.interceptors.response.use(
	    function (response) {
	        return response.data;
	    },
	    function (error) {
	        if (error.response && error.response.data) {
	            if (error.response.data.message) {
	                const message = error.response.data.message;
	                return Promise.reject({ message });
	            } else return Promise.reject(error.response.data);
	        } else {
	            return Promise.reject({
	                message: 'Some unusual error occured, please try again',
	            });
	        }
	    },
	);
