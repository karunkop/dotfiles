3b18ad1df608dec2c7703fad6a5ab8df|file:///home/prabhat/Documents/code/kalysys/client/src/components/DisplayActivitiesLog/DisplayActivities.js|43|1|V|javascript
	    const renderEntryViewPopup = () => {
	        if (!activeEntry) {
	            return null;
	        }
	        let popupEntry;
	        if (activeEntry.source === "nested") {
	            popupEntry = replies[activeEntry.sourceInfo.parentEntryId].repliesEntryInfo[activeEntry.entryId];
	        } else {
	            popupEntry = state.relation.entriesInfo[activeEntry.entryId];
	        }
	        let popupRecord = state.relation.recordsInfo[activeEntry.recordId];
	        const canViewFullScreen = state.relation.moduleRecords.includes(popupRecord.recordValueId);
	
	        //body of the popup
	        let body = null;
	        let isEmailRecord = popupRecord && popupRecord.internalid === "email";
	        let isBodyLoading = !viewEntry[activeEntry.recordId] || viewEntry[activeEntry.recordId].loading;
	        if (isBodyLoading) {
	            body = <InlineLoader />;
	        } else if (viewEntry[activeEntry.recordId].error) {
	            body = (
	                <div className="d-flex align-items-center justify-content-between mb-2">
	                    <div className="display-7">
	                        <i className="fas fa-exclamation text-danger mr-2" />
	                        Cannot display entry
	                    </div>
	                </div>
	            );
	        } else if (isEmailRecord) {
	            body = (
	                <EmailBody
	                    userEmail={props.roles.employee.email}
	                    source={activeEntry.source}
	                    sourceInfo={activeEntry.sourceInfo}
	                    emailEntryInfo={popupEntry}
	                    emailRecordInfo={popupRecord}
	                    changeActiveActivity={handleActivityClick}
	                    onInternalReplySuccessful={onInternalReplySuccessful}
	                />
	            );
	        } else {
	            //if successfully loaded data required to view the entry
	            const viewEntryInfo = viewEntry[activeEntry.recordId];
	            const form = viewEntryInfo.formsData.find(f => +f.recordValueId === +popupEntry.formId);
	            const formValues = buildFormValues(popupRecord.fields, form, popupEntry.recordValueId, {}, popupEntry);
	            body = <DisplayEntry form={form} entry={formValues} fields={popupRecord.fields} />;
	        }
	        const cardBodyStyle = {
	            minHeight: "200px",
	            ...((isEmailRecord || isBodyLoading) && { border: "none" }),
	        };
	        return (
	            <RightPanel active={activeEntry} toggle={resetActiveEntry}>
	                <RightPanelHeader toggle={resetActiveEntry}>
	                    <div className="d-flex align-items-start justify-content-between">
	                        <div className="display-7">{popupRecord.name}</div>
	                        {canViewFullScreen && (
	                            <Button
	                                color="secondary"
	                                tag={Link}
	                                to={`/app/records/${activeEntry.recordId}/view/${activeEntry.entryId}`}
	                                style={{ whiteSpace: "nowrap" }}
	                            >
	                                <i className="fal fa-external-link mr-2" />
	                                View Fullscreen
	                            </Button>
	                        )}
	                    </div>
	                </RightPanelHeader>
	
	                <RightPanelBody>
	                    <Card>
	                        <CardBody style={cardBodyStyle}>{body}</CardBody>
	                    </Card>
	                </RightPanelBody>
	            </RightPanel>
	        );
	    };
